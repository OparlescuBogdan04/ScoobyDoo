<Window x:Class="ScoobyDoo.Windows.ComponentInformation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScoobyDoo.Windows"
        mc:Ignorable="d"
        Title="ComponentInformation" Height="280" Width="300" ResizeMode="NoResize">
    <Grid>
        <TextBlock x:Name="_Information" HorizontalAlignment="Center" TextWrapping="Wrap" Text="Information" VerticalAlignment="Center" />
    </Grid>
</Window>

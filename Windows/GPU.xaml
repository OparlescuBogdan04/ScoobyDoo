<Window x:Class="ScoobyDoo.GPU"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScoobyDoo"
        mc:Ignorable="d"
        Title="GPU Testing" Height="450" Width="800" ResizeMode="NoResize">
    <Grid Background="{StaticResource BackgroundGradient}">

        <!--Back Button-->
        <Button Style="{StaticResource BackButton}"/>

        <!--Information Button-->
        <Button x:Name="_GpuInfo" Content="Gpu Info" HorizontalAlignment="Left" Margin="345,370,0,0" VerticalAlignment="Top" Click="_GpuInfo_Click"/>

    </Grid>
</Window>
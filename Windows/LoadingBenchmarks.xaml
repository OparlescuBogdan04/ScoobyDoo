<Window x:Class="ScoobyDoo.Windows.LoadingBenchmarks"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScoobyDoo.Windows"
        mc:Ignorable="d"
        Title="LoadingBenchmarks" Height="450" Width="800">
    <Grid>
        <!--Benchmark name-->
        <TextBlock x:Name="_BenchmarkName" HorizontalAlignment="Center" Margin="0,194,0,0" TextWrapping="Wrap" Text="Benchmark Name" VerticalAlignment="Top"/>

        <!--Time elapsed box-->
        <TextBlock x:Name="_TimeElapsed" HorizontalAlignment="Center" Margin="0,268,0,0" TextWrapping="Wrap" Text="Elapsed Time: " VerticalAlignment="Top"/>
        
        <!--Done-->
        <Button x:Name="_Done" Content="Done" Visibility="Hidden" HorizontalAlignment="Center" Margin="0,380,0,0" VerticalAlignment="Top" Click="_Done_Click"/>
    </Grid>
</Window>
